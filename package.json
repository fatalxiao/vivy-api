{
    "name": "vivy-api",
    "author": "liangxiaojun",
    "version": "3.3.0-alpha.2",
    "description": "A Vivy plugin to handle apis in Vivy model to make api calling easier.",
    "main": "lib/index.js",
    "module": "es/index.js",
    "types": "es/index.d.ts",
    "files": [
        "lib",
        "es",
        "src",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "clean": "rimraf lib es",
        "test": "cross-env NODE_ENV=test jest test/**.test.js",
        "build:commonjs": "tsc --project tsconfig.build.json --module commonjs --declaration false --outDir lib",
        "build:es": "tsc --project tsconfig.build.json --module esnext --declaration true --outDir es",
        "build": "npm run clean && npm run build:commonjs && npm run build:es",
        "do-publish": "npm run build && npm publish"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fatalxiao/vivy-api"
    },
    "keywords": [
        "react",
        "redux",
        "vivy",
        "request-api"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fatalxiao/vivy-api/issues"
    },
    "peerDependencies": {
        "@types/react": "^16.8 || ^17.0 || ^18.0",
        "@types/react-dom": "^16.8 || ^17.0 || ^18.0",
        "react": "^16.8 || ^17.0 || ^18.0",
        "react-dom": "^16.8 || ^17.0 || ^18.0",
        "vivy": "^3"
    },
    "dependencies": {
        "react-vivy": ">=2.2.x"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.1",
        "@types/react": ">=16.8",
        "@types/react-dom": ">=16.8",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "axios": "^1.3.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.38.0",
        "eslint-plugin-jest": "^27.2.1",
        "express": "^4.18.2",
        "jest": "^29.5.0",
        "react": ">=16.8.x",
        "react-dom": ">=16.8.x",
        "rimraf": "^5.0.0",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.4",
        "vivy": "^3.3.2"
    }
}
