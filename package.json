{
    "name": "vivy-api",
    "author": "liangxiaojun",
    "version": "3.1.0",
    "description": "A Vivy plugin to handle apis in Vivy model to make api calling easier.",
    "main": "lib/index.js",
    "module": "es/index.js",
    "types": "es/index.d.ts",
    "files": [
        "lib",
        "es",
        "src",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "clean": "rimraf lib es",
        "test": "cross-env NODE_ENV=test jest test/**.test.js",
        "build:commonjs": "tsc --module commonjs --declaration false --outDir lib",
        "build:es": "tsc --module esnext --declaration true --outDir es",
        "build": "npm run clean && npm run build:commonjs && npm run build:es",
        "do-publish": "npm run build && npm publish"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fatalxiao/vivy-api"
    },
    "keywords": [
        "react",
        "redux",
        "vivy",
        "request-api"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fatalxiao/vivy-api/issues"
    },
    "dependencies": {
        "react-vivy": ">=2.2.x"
    },
    "devDependencies": {
        "axios": "^1.3.4",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "eslint": "^8.36.0",
        "eslint-plugin-jest": "^27.2.1",
        "express": "^4.18.2",
        "jest": "^29.5.0",
        "rimraf": "^4.4.0",
        "typescript": "^4.9.5",
        "vivy": "^3.2.0"
    }
}
